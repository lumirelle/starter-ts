{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "description": "_description_",
  "author": "Lumirelle <shabbyacc@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/lumirelle",
  "homepage": "https://github.com/lumirelle/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumirelle/pkg-placeholder.git"
  },
  "bugs": "https://github.com/lumirelle/pkg-placeholder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "bun run simple-git-hooks",
    "dev": "bun run tsdown --watch",
    "build": "bun run tsdown",
    "start": "bun run ./src/index.ts",
    "play": "bun run --cwd playground play",
    "lint": "bun run eslint",
    "typecheck": "bun run tsc --noEmit",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "prerelease": "bun run test && bun run typecheck",
    "release": "bun run bumpp"
  },
  "workspaces": {
    "packages": [
      "playground",
      "packages/*",
      "examples/*"
    ],
    "catalogs": {
      "dev": {
        "@antfu/eslint-config": "^6.7.3",
        "bumpp": "^10.3.2",
        "eslint": "^9.39.2",
        "eslint-plugin-format": "^1.1.0",
        "lint-staged": "^16.2.7",
        "simple-git-hooks": "git+https://github.com/toplenboren/simple-git-hooks.git#7625657",
        "tsdown": "^0.18.4",
        "typescript": "^5.9.3",
        "vite": "^7.3.0"
      },
      "inlined": {
        "@antfu/utils": "^9.3.0"
      },
      "testing": {
        "vitest-package-exports": "^1.1.1"
      },
      "types": {
        "@types/bun": "^1.3.5"
      }
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/utils": "catalog:inlined",
    "@types/bun": "catalog:types",
    "bumpp": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "lint-staged": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vitest-package-exports": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run lint-staged"
  },
  "lint-staged": {
    "*": "bun run eslint --fix"
  }
}
