{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "description": "_description_",
  "author": "Lumirelle <shabbyacc@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/lumirelle",
  "homepage": "https://github.com/lumirelle/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumirelle/pkg-placeholder.git"
  },
  "bugs": "https://github.com/lumirelle/pkg-placeholder/issues",
  "keywords": [],
  "workspaces": {
    "packages": [
      "."
    ],
    "catalogs": {
      "inlined": {
        "@antfu/utils": "^9.3.0"
      },
      "dev": {
        "bumpp": "^10.4.0",
        "simple-git-hooks": "git+https://github.com/toplenboren/simple-git-hooks.git#7625657",
        "typescript": "^5.9.3"
      },
      "types": {
        "@types/bun": "^1.3.7"
      },
      "build": {
        "bunup": "^0.16.20"
      },
      "check": {
        "@antfu/eslint-config": "^7.2.0",
        "@arethetypeswrong/cli": "^0.18.2",
        "eslint": "^9.39.2",
        "eslint-plugin-oxlint": "^1.50.0",
        "knip": "^5.82.1",
        "nano-staged": "^0.9.0",
        "oxfmt": "^0.35.0",
        "oxlint": "^1.50.0",
        "publint": "^0.3.17"
      },
      "test": {
        "vitest-package-exports": "^1.1.2"
      }
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "bun simple-git-hooks",
    "dev": "bunup --watch",
    "build": "bunup",
    "start": "bun ./src/index.ts",
    "format": "bunx oxfmt",
    "lint": "bun --parallel oxlint eslint",
    "lint:fix": "bun --sequential 'oxlint --fix' 'eslint --fix'",
    "check:type": "bun tsc --noEmit",
    "check:usage": "bun knip",
    "check:dist": "bun --parallel 'attw --pack --profile esm-only' publint",
    "check": "bun run --parallel lint check:type check:usage check:dist",
    "test": "bun test",
    "prerelease": "bun run --sequential build check test",
    "release": "bun bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:check",
    "@antfu/utils": "catalog:inlined",
    "@arethetypeswrong/cli": "catalog:check",
    "@types/bun": "catalog:types",
    "bumpp": "catalog:dev",
    "bunup": "catalog:build",
    "eslint": "catalog:check",
    "eslint-plugin-oxlint": "catalog:check",
    "knip": "catalog:check",
    "nano-staged": "catalog:check",
    "oxfmt": "catalog:check",
    "oxlint": "catalog:check",
    "publint": "catalog:check",
    "simple-git-hooks": "catalog:dev",
    "typescript": "catalog:dev",
    "vitest-package-exports": "catalog:test"
  },
  "simple-git-hooks": {
    "pre-commit": "bun nano-staged"
  },
  "nano-staged": {
    "*": [
      "bun oxlint",
      "bun eslint",
      "bunx oxfmt --no-error-on-unmatched-pattern"
    ]
  }
}
