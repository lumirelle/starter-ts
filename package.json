{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "description": "_description_",
  "author": "Lumirelle <shabbyacc@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/lumirelle",
  "homepage": "https://github.com/lumirelle/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumirelle/pkg-placeholder.git"
  },
  "bugs": "https://github.com/lumirelle/pkg-placeholder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "tsdown --watch",
    "build": "tsdown",
    "start": "bun src/index.ts",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "prerelease": "bun run test run && bun run typecheck",
    "release": "bumpp"
  },
  "workspaces": {
    "packages": [
      "playground",
      "packages/*",
      "examples/*"
    ],
    "catalogs": {
      "dev": {
        "@antfu/eslint-config": "^6.2.0",
        "bumpp": "^10.3.1",
        "czg": "^1.12.0",
        "eslint": "^9.39.1",
        "eslint-plugin-format": "^1.0.2",
        "lint-staged": "^16.2.7",
        "simple-git-hooks": "^2.13.1",
        "taze": "^19.9.0",
        "tsdown": "^0.16.6",
        "typescript": "^5.9.3",
        "vite": "^7.2.4"
      },
      "inlined": {
        "@antfu/utils": "^9.3.0"
      },
      "testing": {
        "tinyexec": "^1.0.2",
        "vitest-package-exports": "^0.1.1",
        "yaml": "^2.8.1"
      },
      "types": {
        "@types/bun": "^1.3.3"
      }
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/utils": "catalog:inlined",
    "@types/bun": "catalog:types",
    "bumpp": "catalog:dev",
    "czg": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "lint-staged": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "taze": "catalog:dev",
    "tinyexec": "catalog:testing",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "na exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
